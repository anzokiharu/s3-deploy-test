{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    //"strict": false,
    "paths": {
      "@/*": ["src/*"],
      "@core/*": ["src/assets/scripts/core/*"],
      "@lib/*": ["src/assets/scripts/lib/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
